{
  "name": "@odopod/odo",
  "description": "A collection of vanilla JavaScript components used in Odopod projects.",
  "private": true,
  "scripts": {
    "build-test-all": "./scripts/run-in-each-package.sh \"gulp\"",
    "docs": "node ./scripts/docs-generate.js",
    "install-all": "yarn install && ./scripts/run-in-each-package.sh \"yarn install\"",
    "link": "yarn run link:prep && yarn run link:local",
    "link:local": "node ./scripts/link-all-local.js",
    "link:prep": "./scripts/run-in-each-package.sh \"yarn link\"",
    "link:unlink-local": "node ./scripts/unlink-all-local.js",
    "link-install": "yarn run link && yarn run install-all",
    "setup": "yarn run link-install && yarn run build-test-all",
    "test": "./scripts/run-in-each-package.sh \"yarn test\""
  },
  "dependencies": {
    "@odopod/eslint-config": "^1.1.0",
    "ansi-escapes": "^2.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^4.1.0",
    "classlist.js": "github:allannienhuis/classList.js",
    "eslint": "^4.3.0",
    "fs-extra": "^4.0.0",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-istanbul-report": "^0.0.1",
    "gulp-mocha-phantomjs": "^0.12.0",
    "gulp-sequence": "^0.4.6",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "mocha-phantomjs": "^4.1.0",
    "mocha-phantomjs-istanbul": "^0.0.2",
    "nunjucks": "^3.0.0",
    "pify": "^3.0.0",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "shufflejs": "^5.0.0",
    "sinon": "^2.3.8"
  }
}
